include ./menu-data

header.main
  .header-top
    .windermere-logo
      img(src=themeDir + "images/windmere-logo.png" alt="Windermere Logo")
    .logo
      include icons/logo.svg
    .contact-block
      a(href='tel:1-206-409-2244').contact-link
        .icon
          include icons/phone.svg
        span 1-206-409-2244
      a(href='mailto:thebeuschers@windermere.com').contact-link
        .icon
          include icons/envelop.svg
        span.email thebeuschers@windermere.com
  nav.main-nav
    for item in menudata
      a(href=item.url, class=item.classname)= item.label